# Wallet Provider Configuration
# Options: 'para' (default) or 'rainbow'
# Para: Simple social login wallet
# Rainbow: Traditional crypto wallets (MetaMask, Coinbase, etc.)
NEXT_PUBLIC_WALLET_PROVIDER=para

# Network Configuration
# Set to true for Moksha testnet, false/unset for mainnet
NEXT_PUBLIC_MOKSHA=true

# Google OAuth Configuration (required for Google Drive storage)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here

# Application Addresses
NEXT_PUBLIC_DATA_WALLET_APP_ADDRESS=0x...
NEXT_PUBLIC_DEFAULT_GRANTEE_ID=1

# Schema Configuration (optional)
NEXT_PUBLIC_VIBES_SCHEMA_ID=19

# Wallet Connect Project ID (optional, for Rainbow wallet)
NEXT_PUBLIC_REOWN_PROJECT=your_project_id_here

# Para API Key (optional, defaults provided)
NEXT_PUBLIC_PARA_KEY=your_para_key_here