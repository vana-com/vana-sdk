{
  "name": "@vana/load-test",
  "version": "1.0.0",
  "description": "Load testing suite for Vana SDK data portability workflow",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "prepare-wallets": "tsx src/scripts/prepare-wallets.ts",
    "load-test:burst": "tsx src/scripts/run-test.ts --preset=burst",
    "load-test:conservative": "tsx src/scripts/run-test.ts --preset=conservative",
    "load-test:custom": "tsx src/scripts/run-test.ts",
    "monitor-results": "tsx src/scripts/monitor.ts",
    "clean": "rm -rf dist",
    "artillery:burst": "artillery run configs/burst-test.yml",
    "artillery:conservative": "artillery run configs/conservative-test.yml"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "@google-cloud/storage": "^7.17.0",
    "@opendatalabs/vana-sdk": "file:../packages/vana-sdk",
    "artillery": "^2.0.20",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.17.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "vana",
    "load-testing",
    "performance",
    "blockchain",
    "data-portability"
  ],
  "author": "Vana Team",
  "license": "MIT"
}
