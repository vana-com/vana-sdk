{
  "name": "vana-sdk",
  "version": "1.0.9",
  "description": "A TypeScript library for interacting with Vana Network smart contracts.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./chains": {
      "types": "./dist/config/chains.d.ts",
      "import": "./dist/config/chains.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "fetch-abis": "tsx scripts/fetch-abis.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vana",
    "blockchain",
    "web3",
    "sdk"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "abitype": "^1.0.8",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eccrypto": "^1.1.6",
    "openpgp": "^6.1.1",
    "viem": "^2.31.7"
  },
  "devDependencies": {
    "@types/eccrypto": "^1.1.6",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.3.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vana-com/vana-sdk.git",
    "directory": "packages/vana-sdk"
  }
}
