{
  "name": "vana-sdk",
  "version": "1.0.9",
  "description": "A TypeScript library for interacting with Vana Network smart contracts.",
  "type": "module",
  "main": "dist/index.node.js",
  "types": "dist/index.node.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/index.node.js",
        "types": "./dist/index.node.d.ts"
      },
      "browser": {
        "import": "./dist/index.browser.js",
        "types": "./dist/index.browser.d.ts"
      },
      "default": {
        "import": "./dist/index.node.js",
        "types": "./dist/index.node.d.ts"
      }
    },
    "./platform/*": {
      "import": "./dist/platform/*.js",
      "types": "./dist/platform/*.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./chains": {
      "import": "./dist/chains.js",
      "types": "./dist/chains.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "fetch-abis": "tsx scripts/fetch-abis.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vana",
    "blockchain",
    "web3",
    "sdk"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "abitype": "^1.0.8",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eccrypto": "^1.1.6",
    "eccrypto-js": "^5.4.0",
    "openpgp": "^6.1.1",
    "viem": "^2.31.7"
  },
  "devDependencies": {
    "@types/eccrypto": "^1.1.6",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.3.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vana-com/vana-sdk.git",
    "directory": "packages/vana-sdk"
  }
}
