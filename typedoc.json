{
  "entryPoints": ["packages/vana-sdk/src"],
  "entryPointStrategy": "expand",
  "exclude": [
    "packages/vana-sdk/src/abi/**/*",
    "packages/vana-sdk/src/contracts/**/*",
    "packages/vana-sdk/src/types/server.ts"
  ],
  "out": "docs",
  "name": "Vana SDK",
  "readme": "packages/vana-sdk/README.md",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeNotDocumented": true,
  "categorizeByGroup": true,
  "defaultCategory": "Reference",
  "categoryOrder": [
    "Core SDK",
    "Data Management",
    "Permissions",
    "Storage",
    "Configuration",
    "Advanced",
    "Reference",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": ["Class", "Function", "Variable", "TypeAlias"],
  "searchInComments": true,
  "includeVersion": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "skipErrorChecking": true
}
